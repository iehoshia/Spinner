<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.holamundo.app.holamundo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
        </activity>
        <activity android:name=".CycleActivity" >
        </activity>
        <activity android:name=".UsuarioActivity" >
        </activity>
        <activity
            android:name=".StudentActivity"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.MainActivity" >
            </meta-data>
        </activity>
        <activity android:name=".TeacherActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.MainActivity" >
            </meta-data>
        </activity>
        <activity android:name=".CourseActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.MainActivity" >
            </meta-data>
        </activity>
        <activity android:name=".Main2Activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SpinnerTestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.MainActivity" >
            </meta-data>
        </activity>
        <activity
            android:name=".AddTeacherActivity"
            android:label="@string/title_activity_add_teacher"
            android:parentActivityName=".Main2Activity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.TeacherActivity" />
        </activity>
        <activity
            android:name=".AddCourseActivity"
            android:label="@string/title_activity_add_course"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.holamundo.app.holamundo.CourseActivity" >
            </meta-data>
        </activity>
        <activity
            android:name=".StudentCourseActivity"
            android:label="@string/title_activity_student_course"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".AddStudentCourseActivity"
            android:label="@string/title_activity_add__student_course"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
